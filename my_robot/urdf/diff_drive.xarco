<?xml version="1.0"?>

<!-- choosing namespace- start xarco-->
<robot name="diff_drive" xmlns:xacro="http://www.ros.org/wiki/xacro">

  <!-- Including required xarco-->
  <xacro:include filename="$(find my_robot)/urdf/rb_xarco.xarco" />
  <!-- Create dummy link as base link-->
  <link name="base_link">
    <pose>  0 0 0 0 0 0 </pose>
  </link>

  <joint name="base_joint" type="fixed">
    <parent link="base_link"/>
    <child link="diff_base"/>
  </joint>

  <!-- DIFF_BASE LINK-->
  <link name = "diff_base">
    <pose> 0 0 0 0 0 0 </pose>
    <collision>
      <origin xyz = "-${box_x / 2} ${box_y / 2} 0 " rpy = " 1.5708  0 0" />
        <geometry>
          <mesh filename="package://my_robot/meshes/base.stl" scale = "0.001 0.001 0.001"/>
      </geometry>
    </collision>

    <visual>
      <origin xyz = "-${box_x / 2} ${box_y / 2} 0 "  rpy = " 1.5708  0 0" />
      <geometry>
        <mesh filename="package://my_robot/meshes/base.stl" scale = "0.001 0.001 0.001"/>
      </geometry>
    </visual>

    <inertial>
      <mass  value = "${box_mass}"/>
      <inertia
       ixx = "${box_xx}"
       iyy = "${box_yy}"
       izz = "${box_zz}"
       ixy = "0"
       ixz = "0"
       iyz = "0"
       />
    </inertial>
  </link>

  <!-- FRONT WHEEL LINK  JOINT-->
  <link name="fwheel">
    <collision>
      <origin xyz = "0 0 0" rpy = "-1.5708 0 0 "/>
      <geometry>
        <mesh filename="package://my_robot/meshes/fwheel.stl" scale = "0.001 0.001 0.001"/>
      </geometry>
      <surface>
        <friction>
          <ode>
            <slip1>1.0</slip1>
            <slip2>1.0</slip2>
          </ode>
        </friction>
      </surface>
    </collision>

    <visual>
      <origin xyz = "0 0 0" rpy = "-1.5708 0 0 "/>
      <geometry>
        <mesh filename="package://my_robot/meshes/fwheel.stl" scale = "0.001 0.001 0.001"/>
      </geometry>
    </visual>

    <inertial>
      <mass  value = "${sphere_mass}"/>
      <inertia
       ixx = "${sphere_xx}"
       iyy = "${sphere_yy}"
       izz = "${sphere_zz}"
       ixy = "0"
       ixz = "0"
       iyz = "0"
       />
    </inertial>
  </link>


  <joint name = "base_to_fwheel" type = "fixed">
    <parent link = "diff_base"/>
    <child link = "fwheel"/>
    <origin xyz = "${box_x / 2} 0 -${box_z / 2}"/>
    <preserveFixedJoint>true</preserveFixedJoint>
  </joint>

  <!-- LEFT WHEEL LINK-->
  <link name = "lwheel">
    <collision>
      <origin xyz = "-${cylinder_radius} 0 -${cylinder_radius}" rpy = "1.5708 0 0" />
      <geometry>
        <mesh filename="package://my_robot/meshes/wheel.stl"  scale = "0.001 0.001 0.001"/>
      </geometry>
    </collision>

    <visual>
      <origin xyz = "-${cylinder_radius} 0 -${cylinder_radius}" rpy = "1.5708 0 0" />
      <geometry>
        <mesh filename= "package://my_robot/meshes/wheel.stl"  scale = "0.001 0.001 0.001"/>
      </geometry>
    </visual>

    <inertial>
      <mass value = "${cylinder_mass}"/>
      <inertia
        ixx = "${cylinder_xx}"
        iyy = "${cylinder_yy}"
        izz = "${cylinder_zz}"
        ixy = "0"
        ixz = "0"
        iyz = "0"  
      />      
    </inertial>
  </link>

  <joint name = "base_to_lwheel" type = "continuous">
    <parent link = "diff_base"/>
    <child link = "lwheel"/>
    <origin xyz = "-${box_x / 4} ${2* box_y / 5} -${box_z / 2}"/>
    <axis xyz = " 0 1 0"/>
    <limit effort = "100" velocity="100"/>
  </joint>

<!-- RIGHT WHEEL LINK-->
  <link name = "rwheel">
    <collision>
      <origin xyz = "-${cylinder_radius} 0 -${cylinder_radius}" rpy = "1.5708 0 0" />
      <geometry>
        <mesh filename="package://my_robot/meshes/wheel.stl" scale = "0.001 0.001 0.001"/>
      </geometry>
    </collision>

    <visual>
      <origin xyz = "-${cylinder_radius} 0 -${cylinder_radius}" rpy = "1.5708 0 0" />
      <geometry>
        <mesh filename="package://my_robot/meshes/wheel.stl" scale = "0.001 0.001 0.001"/>
      </geometry>
    </visual>

    <inertial>
      <mass value = "${cylinder_mass}"/>
      <inertia
        ixx = "${cylinder_xx}"
        iyy = "${cylinder_yy}"
        izz = "${cylinder_zz}"
        ixy = "0"
        ixz = "0"
        iyz = "0"  
      />      
    </inertial>
  </link>

  <joint name = "base_to_rwheel" type = "continuous">
    <parent link = "diff_base"/>
    <child link = "rwheel"/>
    <origin xyz = "-${box_x / 4} -${2* box_y / 5} -${box_z / 2}"/>
    <axis xyz = " 0 1 0"/>
    <limit effort = "100" velocity="100"/>
  </joint>

<!-- HOKUYO LASER LINK -->
  <link name = "laser_link">
    <collision>
      <origin xyz = "0 0 0" rpy = "1.5708 0 0"/>
      <geometry>
        <sphere radius = "${hokuyo_radius}" length = "${hokuyo_height}"/>
      </geometry>
    </collision>

    <visual>
      <origin xyz = "0 0 0" rpy = "1.5708 0 0"/>
      <geometry>
        <sphere radius = "${hokuyo_radius}" length = "${hokuyo_height}"/>
      </geometry>
    </visual>

    <inertial>
      <mass value = "1e-5"/>
      <inertia
        ixx = "${hokuyo_xx}"
        iyy = "${hokuyo_yy}"
        izz = "${hokuyo_zz}"
        ixy = "0"
        ixz = "0"
        iyz = "0" 
      /> 
    </inertial>
  </link>

  <joint name = "hokuyo_joint" type = "fixed">
    <parent link = "diff_base"/>
    <child link = "laser_link"/>
    <axis xyz = "0 1 0"/>
    <origin xyz = "0 0 ${box_z / 2}" rpy = " 0 0 0"/>
  </joint>

<!-- HOKUYO LASER GAZEBO PLUGIN-->
<gazebo reference = "laser_link">
  <sensor type = "ray" name = "head_hokuyo_sensor">
    <pose>0 0 0 0 0 0 </pose>
    <visualize>true</visualize>
    <update_rate>20</update_rate>
    <ray>
      <scan>
        <horizontal>
          <sample>720</sample>
          <resolution>1</resolution>
          <min_angle>-1.570796</min_angle>
          <max_angle>1.570796</max_angle>
        </horizontal>
      </scan>
      <range>
        <min>0.1</min>
        <max>10.0</max>
        <resolution>0.01</resolution>
      </range>
      <noise>
        <type>gaussian</type>
        <mean>0.0</mean>
        <stddev>0.01</stddev> 
      </noise>
    </ray>
    <plugin name="laser" filename="libgazebo_ros_laser.so">
      <topicName>/scan</topicName>
      <frameName>/laser_link</frameName>
    </plugin>
  </sensor>
</gazebo>
